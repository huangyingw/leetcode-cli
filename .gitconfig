[remote "skygragon"]
    url = https://github.com/skygragon/leetcode-cli.git
    fetch = +refs/heads/*:refs/remotes/skygragon/*
[remote "origin"]
    url = git@bitbucket.org:huangyingw/leetcode-cli.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "gitlab"]
    url = git@gitlab.com:huangyingw/leetcode-cli.git
    fetch = +refs/heads/*:refs/remotes/gitlab/*
[remote "github"]
    url = https://github.com/huangyingw/leetcode-cli.git 
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = origin
    remote = gitlab
[gsync]
    remote = skygragon
    target = skygragon/master
[branch "lc/dev"]
    remote = origin
    merge = refs/heads/lc/dev
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[gdif]
    ignore = *.md
    ignore = *.sh
    ignore = .gitignore
    ignore = .gitmodules
    ignore = companies.list
    ignore = downloads
    ignore = error
    ignore = excludeFile
    ignore = fdocs.list
    ignore = lib/plugins/company.js
    ignore = lib/plugins/cookie.chrome.js
    ignore = lib/plugins/solution.discuss.js
    ignore = package-lock.json
    ignore = problems.list
    ignore = readme
    ignore = remove_comments
    ignore = setup.cfg
    ignore = show
    ignore = sort_accepts.py
    ignore = sort_non_accepts.py
    ignore = submissions
    ignore = tags.list
    ignore = tags/*
[deploy]
    host = localhost
    path = ~/myproject/git/interview/skygragon/leetcode-cli/
[branch "master"]
    remote = origin
    merge = refs/heads/master
